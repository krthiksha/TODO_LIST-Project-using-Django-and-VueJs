{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "public": "dist",
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  